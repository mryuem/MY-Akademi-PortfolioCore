@model List<SocialAccount>


<section id="hero" class="hero section light-background">

    <img src="/MyResume-1.0.0/assets/img/hero-bg.jpg" alt="">

    <div class="container" data-aos="zoom-out">
        <div class="row justify-content-center">
            <div class="col-lg-9">
                <h2>Yunus Emre Altun</h2>
                <p> <span class="typed" data-typed-items="FullStack Asp.Net Core Developer"></span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
                <div class="social-links">


                @foreach(var item in Model)
                {
                            <a href="@item.SocialUrl"><i class="@item.Icon.IconClass"></i></a>
                       
                }
                    </div>


            </div>
        </div>
    </div>

</section><!-- /Hero Section -->
