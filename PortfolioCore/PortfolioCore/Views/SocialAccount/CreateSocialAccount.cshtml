 @model SocialAccount
@{
    ViewData["Title"] = "CreateSocialAccount";
    Layout = "~/Views/AdminLayout/Index.cshtml";
   
}

<h2>Yeni Sosyal Medya Hesabı Ekle</h2>

<form method="post">
    
    <label>Sosyal Medya Url</label>
    <input type="text" asp-for="SocialUrl" class="form-control" />
    <br />
    <label>Sosyal Medya Icon</label>

    <select class="form-control" asp-for="IconId" asp-items="ViewBag.v">
        <option>--- Seciçiniz ---</option>
    </select>
    <br />
    <button type="submit" class="btn btn-success">Ekle</button>
</form> 