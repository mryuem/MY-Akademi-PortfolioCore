@model Contact
@{
    ViewData["Title"] = "UpdateEducation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>İletişim Güncelleme Sayfası</h1>

<form method="post">
    <input type="hidden" asp-for="ContactId" />
    <label>Başlık</label>
    <input type="text" class="form-control" asp-for="Address" style="border:1px solid gray;" />
    <br />
    <label>Alt Başlık</label>
    <input type="text" class="form-control" asp-for="PhoneNumber" style="border:1px solid gray;" />
    <br />
    <label>Tarih</label>
    <input type="text" class="form-control" asp-for="Email" style="border:1px solid gray;" />
    <br />
    <label>Açıklama</label>
    <input type="text" class="form-control" asp-for="Location" style="border:1px solid gray;" />
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>

