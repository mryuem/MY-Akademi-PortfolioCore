@model Contact
@{
    ViewData["Title"] = "CreateEducation";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>İletişim Ekleme Sayfası</h2>

<form method="post">

    <label>Başlık</label>
    <input type="text" asp-for="Address" style="border:1px solid gray;" class="form-control" />
    <br />
    <label>Tarih</label>
    <input type="text" asp-for="PhoneNumber" style="border:1px solid gray;" class="form-control" />
    <br />
    <label>Alt Başlık</label>
    <input type="text" asp-for="Email" style="border:1px solid gray;" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="Location" style="border:1px solid gray;" class="form-control" />
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>