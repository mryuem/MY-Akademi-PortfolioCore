@model Skill
@{
    ViewData["Title"] = "CreateSkill";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Yetenek Oluştur</h2>

<form  method="post">
    <label>Yetenek Adı</label>
    <input type="text" class="form-control" asp-for="SkillName" />
    <br />
    <label>Yetenek Değeri</label>
    <input type="number" class="form-control" asp-for="SkillValue" />
    <br />
    <button class="btn btn-primary">Yeni Yetenek Oluştur</button>
</form>
